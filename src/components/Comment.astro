---
import { commentConfig } from '@/config';
import Giscus from '@components/comment/Giscus.astro';
import Twikoo from '@components/comment/Twikoo.astro';
---

<div id="page-comment">
  {
    (() => {
      switch (commentConfig.provider) {
        case 'twikoo':
          return <Twikoo />;
        case 'giscus':
          return <Giscus />;
      }
    })()
  }
</div>
